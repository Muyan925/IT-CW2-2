{"ast":null,"code":"import InputBox from '@/components/InputBox';\nexport default {\n  name: 'LoginPage',\n  components: {\n    InputBox\n  },\n  data() {\n    return {\n      phoneNumber: '',\n      password: ''\n    };\n  },\n  methods: {\n    login() {\n      this.$axios.get(`/login?userId=${this.phoneNumber}&userPassword=${this.password}`).then(response => {\n        console.log(response.data);\n        if (response.data.flag) {\n          console.log(response.data);\n          this.$message.success(response.data.msg);\n          this.$store.dispatch('saveUserInfo', response.data.data);\n          this.timer = setTimeout(() => {\n            this.$router.replace('/manage/index');\n          }, 1500);\n        } else {\n          this.$message.error(response.data.msg);\n        }\n      });\n    }\n  },\n  beforeDestroy() {\n    clearTimeout(this.timer);\n  }\n};","map":{"version":3,"mappings":"AAiBA;AACA;EACAA;EACAC;IACAC;EACA;EACAC;IACA;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACA;QACAC;QACA;UACAA;UACA;UACA;UACA;YACA;UACA;QACA;UACA;QACA;MACA;IACA;EACA;EACAC;IACAC;EACA;AACA","names":["name","components","InputBox","data","phoneNumber","password","methods","login","console","beforeDestroy","clearTimeout"],"sourceRoot":"src/pages","sources":["LoginPage.vue"],"sourcesContent":["<template>\n  <div class=\"login\">\n    <div class=\"form-box\">\n        <div class=\"info\">\n            <div class=\"title\">易启智学</div>\n            <div class=\"description\">笔记管理系统</div>\n        </div>\n        <div class=\"form\">\n            <input type=\"text\">\n            <input type=\"text\">\n            <button class=\"btn\">登陆</button>\n        </div>\n    </div>\n  </div>\n</template>\n\n<script lang=\"js\">\nimport InputBox from '@/components/InputBox'\nexport default {\n    name: 'LoginPage',\n    components: {\n        InputBox\n    },\n    data() {\n        return {\n            phoneNumber: '',\n            password: ''\n        }\n    },\n    methods: {\n        login() {\n            this.$axios.get(`/login?userId=${this.phoneNumber}&userPassword=${this.password}`).then(response => {\n                console.log(response.data);\n                if (response.data.flag) {\n                    console.log(response.data);\n                    this.$message.success(response.data.msg)\n                    this.$store.dispatch('saveUserInfo', response.data.data)\n                    this.timer = setTimeout(() => {\n                        this.$router.replace('/manage/index')\n                    }, 1500);\n                } else {\n                    this.$message.error(response.data.msg)\n                }\n            })\n        }\n    },\n    beforeDestroy() {\n        clearTimeout(this.timer)\n    }\n}\n</script>\n\n<style scoped>\n.form-box {\n    width: 600px;\n    height: 300px;\n    background-color: pink;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}