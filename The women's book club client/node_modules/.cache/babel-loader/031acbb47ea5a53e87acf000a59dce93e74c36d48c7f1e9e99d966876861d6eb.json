{"ast":null,"code":"import LoginForm from '@/components/LoginForm';\nimport RegisterForm from '@/components/RegisterForm';\nexport default {\n  name: 'LoginPage',\n  components: {\n    LoginForm,\n    RegisterForm\n  },\n  data() {\n    return {\n      phoneNumber: '',\n      password: ''\n    };\n  },\n  methods: {\n    login() {\n      this.$axios.get(`/login?userId=${this.phoneNumber}&userPassword=${this.password}`).then(response => {\n        console.log(response.data);\n        if (response.data.flag) {\n          console.log(response.data);\n          this.$message.success(response.data.msg);\n          this.$store.dispatch('saveUserInfo', response.data.data);\n          this.timer = setTimeout(() => {\n            this.$router.replace('/manage/index');\n          }, 1500);\n        } else {\n          this.$message.error(response.data.msg);\n        }\n      });\n    }\n  },\n  beforeDestroy() {\n    clearTimeout(this.timer);\n  }\n};","map":{"version":3,"mappings":"AAmBA;AACA;AACA;EACAA;EACAC;IACAC;IAAAC;EACA;EACAC;IACA;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACA;QACAC;QACA;UACAA;UACA;UACA;UACA;YACA;UACA;QACA;UACA;QACA;MACA;IACA;EACA;EACAC;IACAC;EACA;AACA","names":["name","components","LoginForm","RegisterForm","data","phoneNumber","password","methods","login","console","beforeDestroy","clearTimeout"],"sourceRoot":"src/pages","sources":["LoginPage.vue"],"sourcesContent":["<template>\n  <div class=\"login\">\n    <div class=\"form-box\">\n        <div class=\"info\">\n            <div class=\"title\">易启智学</div>\n            <div class=\"description\">笔记管理系统</div>\n        </div>\n        <div class=\"form\">\n            <RegisterForm></RegisterForm>\n        </div>\n        <div class=\"form\">\n            <LoginForm></LoginForm>\n            \n        </div>\n    </div>\n  </div>\n</template>\n\n<script lang=\"js\">\nimport LoginForm from '@/components/LoginForm'\nimport RegisterForm from '@/components/RegisterForm'\nexport default {\n    name: 'LoginPage',\n    components: {\n        LoginForm, RegisterForm\n    },\n    data() {\n        return {\n            phoneNumber: '',\n            password: ''\n        }\n    },\n    methods: {\n        login() {\n            this.$axios.get(`/login?userId=${this.phoneNumber}&userPassword=${this.password}`).then(response => {\n                console.log(response.data);\n                if (response.data.flag) {\n                    console.log(response.data);\n                    this.$message.success(response.data.msg)\n                    this.$store.dispatch('saveUserInfo', response.data.data)\n                    this.timer = setTimeout(() => {\n                        this.$router.replace('/manage/index')\n                    }, 1500);\n                } else {\n                    this.$message.error(response.data.msg)\n                }\n            })\n        }\n    },\n    beforeDestroy() {\n        clearTimeout(this.timer)\n    }\n}\n</script>\n\n<style scoped>\n.login {\n    display: flex;\n    height: 100vh;\n    width: 100vw;\n    justify-content: center;\n    align-items: center;\n    background-image: linear-gradient(to right, rgb(95, 162, 248), rgb(83, 83, 177));\n}\n.form-box {\n    position: relative;\n    display: flex;\n    width: 600px;\n    height: 400px;\n    background-color: #fff;\n    border-radius: 10px;\n    box-shadow: 2px 2px 12px 2px rgba(0, 0, 0, .3);\n}\n\n.info {\n    position: absolute;\n    top: 0;\n    left: 0;\n    height: 100%;\n    display: flex;\n    /* display: none; */\n    flex-direction: column;\n    align-items: center;\n    justify-content: center;\n    width: 50%;\n}\n\n.title {\n    font-size: 34px;\n}\n\n.description {\n    font-size: 14px;\n}\n\n.form {\n    width: 50%;\n    height: 100%;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}